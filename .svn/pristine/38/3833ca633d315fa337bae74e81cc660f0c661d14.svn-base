package com.swz.data.vo.mysql;

import org.springframework.web.multipart.MultipartFile;

public class Info_sound {
	private Integer sound_id;
	private String sound_path;
	private String sound_name;
	private String remark;
	private MultipartFile file;

	public void setSound_id(Integer sound_id) {
		this.sound_id = sound_id;
	}

	public Integer getSound_id() {
		return sound_id;
	}

	public void setSound_path(String sound_path) {
		this.sound_path = sound_path;
	}

	public String getSound_path() {
		return sound_path;
	}

	public void setSound_name(String sound_name) {
		this.sound_name = sound_name;
	}

	public String getSound_name() {
		return sound_name;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public String getRemark() {
		return remark;
	}

	public MultipartFile getFile() {
		return file;
	}

	public void setFile(MultipartFile file) {
		this.file = file;
	}

	@Override
	public String toString() {
		return "{" + "\"sound_id\":\"" + this.sound_id + "\""
				+ ",\"sound_path\":\"" + this.sound_path + "\""
				+ ",\"sound_name\":\"" + this.sound_name + "\""
				+ ",\"remark\":\"" + this.remark + "\"" + "}";
	}

}
